{
  "name": "yunti-server",
  "version": "0.1.0",
  "description": "yunti-server for console",
  "license": "MIT",
  "author": "zhangpc<zhang.pengcheng3@neolink.com>",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "dev": "nest start --watch",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "gen:gql": "zx ./codegen-gql.mjs",
    "lint": "eslint \"{src,apps,packages,libs,test}/**/*.ts\"",
    "lint-fix": "eslint \"{src,apps,packages,libs,test}/**/*.ts\" --fix",
    "prettier": "prettier -c --write \"**/**\"",
    "prettier:ts": "prettier --parser=typescript --write \"{src,apps,packages,libs,test}/**/*.ts\"",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:watch": "jest --watch",
    "typeorm": "typeorm-ts-node-commonjs"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testEnvironment": "node",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    }
  },
  "dependencies": {
    "@apollo/server": "^4.9.3",
    "@graphql-tools/utils": "^10.0.6",
    "@kubernetes/client-node": "^0.19.0",
    "@nestjs/apollo": "^12.0.9",
    "@nestjs/common": "^10.2.5",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.2.5",
    "@nestjs/graphql": "^12.0.9",
    "@nestjs/platform-express": "^10.2.5",
    "@nestjs/platform-ws": "^10.2.5",
    "@nestjs/schedule": "^3.0.4",
    "@nestjs/serve-static": "^4.0.0",
    "@nestjs/typeorm": "^10.0.1",
    "@nestjs/websockets": "^10.2.5",
    "@yuntijs/k8s-client": "workspace:*",
    "apollo-server-errors": "^3.3.1",
    "bing-translate-api": "^2.9.2",
    "body-parser": "^1.20.2",
    "change-case": "^4.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "connect-redis": "^7.1.0",
    "dataloader": "^2.2.2",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "graphql": "^16.8.1",
    "graphql-query-complexity": "^0.12.0",
    "helmet": "^7.0.0",
    "hpagent": "^1.2.0",
    "ioredis": "^5.3.2",
    "isomorphic-ws": "^5.0.0",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "minio": "^7.1.3",
    "moment": "^2.29.4",
    "mysql2": "^3.6.5",
    "nanoid": "^3.3.6",
    "nanoid-dictionary": "^4.3.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.1",
    "rxjs": "^7.8.1",
    "semver": "^7.5.4",
    "typeorm": "^0.3.17",
    "urllib": "^3.19.2",
    "ws": "^8.14.2",
    "yaml": "^2.3.2"
  },
  "devDependencies": {
    "@alilc/lowcode-types": "^1.2.2",
    "@graphql-codegen/cli": "^4.0.1",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-graphql-request": "^5.0.0",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@nestjs/cli": "^10.1.17",
    "@nestjs/schematics": "^10.0.2",
    "@nestjs/testing": "^10.2.5",
    "@types/body-parser": "^1.19.2",
    "@types/ejs": "^3.1.2",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.7",
    "@types/jest": "^29.5.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/lodash": "^4.14.195",
    "@types/minio": "^7.1.1",
    "@types/node": "^18.11.7",
    "@types/semver": "^7.5.0",
    "@types/supertest": "^2.0.12",
    "@types/ws": "^8.5.5",
    "graphql-codegen-plugin-typescript-swr": "0.8.3",
    "jest": "^29.5.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "zx": "^7.2.3"
  },
  "packageManager": "pnpm@8.12.1"
}
